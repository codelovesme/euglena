{
    "name": "euglena",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "compile": "lerna run compile",
        "clean": "lerna run clean && rm -r node_modules",
        "p-e": "(cp -r packages ../euglena-dist)&(cp package.json ../euglena-dist/)"
    },
    "repository": {
        "url": "http://codeloves.me:2800/codelovesme/euglena.git"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "jsxSingleQuote": false,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "always",
        "requirePragma": false,
        "insertPragma": false,
        "proseWrap": "always",
        "endOfLine": "lf"
    },
    "publishConfig": {
        "@codelovesme:registry": "http://codeloves.me:2800/api/v4/projects/3/packages/npm/"
    }
}
